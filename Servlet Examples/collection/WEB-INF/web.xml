<web-app>
<servlet>
    <servlet-name>calculator</servlet-name>
    <servlet-class>CalcServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>sign</servlet-name>
    <servlet-class>SignServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>oddeven</servlet-name>
    <servlet-class>OEServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>fact</servlet-name>
    <servlet-class>FactServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>fibo</servlet-name>
    <servlet-class>FiboServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>calculator</servlet-name>
    <url-pattern>/calc</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>sign</servlet-name>
    <url-pattern>/sign</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>oddeven</servlet-name>
    <url-pattern>/oddeven</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>fact</servlet-name>
    <url-pattern>/fact</url-pattern>
</servlet-mapping>
<servlet-mapping>
    <servlet-name>fibo</servlet-name>
    <url-pattern>/fibo</url-pattern>
</servlet-mapping>
</web-app>
